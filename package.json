{
  "name": "practice_cypress",
  "version": "1.0.0",
  "description": "practice cypress",
  "main": "cypress.config.js",
  "devDependencies": {
    "@cypress/browserify-preprocessor": "^3.0.2",
    "mocha": "^10.2.0"
  },
  "scripts": {
    "open": "npx cypress open",
    "run": "npx cypress run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thdu98/practice_cypress.git"
  },
  "author": "dudu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thdu98/practice_cypress/issues"
  },
  "homepage": "https://github.com/thdu98/practice_cypress#readme",
  "dependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^18.0.4",
    "cypress": "^12.17.4",
    "cypress-xpath": "^2.0.1"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": [
      "cypress/e2e/step_definition/*.js"
    ],
    "nonGlobalStepDefinitions": true,
    "json": {
      "enabled": true,
      "output": "cypress/reports/cucumber-json/cucumber.json"
    },
    "html": {
      "enabled": true,
      "output": "cypress/reports/html/html-report.html"
    }
  }
}
